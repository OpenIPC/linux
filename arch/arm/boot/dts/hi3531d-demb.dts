/dts-v1/;
#include "hi3531d.dtsi"

/ {
	model = "Hisilicon HI3531D DEMO Board";
	compatible = "hisilicon,hi3531d";

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;
		enable-method = "hisilicon,hi3531d-smp";

		cpu@0 {
			compatible = "arm,cortex-a9";
			device_type = "cpu";
			reg = <0>;
			next-level-cache = <&L2>;
		};

		cpu@1 {
			compatible = "arm,cortex-a9";
			device_type = "cpu";
			reg = <1>;
			next-level-cache = <&L2>;
		};
	};

	memory {
		device_type = "memory";
		reg = <0x40000000 0xc0000000>;
	};
};

&uart0 {
	status = "okay";
};

&spi_bus0{
	status = "okay";
	num-cs = <4>;

	spidev@0 {
		compatible = "spidev";
		reg = <0>;
		pl022,interface = <0>;
		pl022,com-mode = <0>;
		spi-max-frequency = <40500000>;
	};

	spidev@1 {
		compatible = "spidev";
		reg = <1>;
		pl022,interface = <0>;
		pl022,com-mode = <0>;
		spi-max-frequency = <40500000>;
	};

	spidev@2 {
		compatible = "spidev";
		reg = <2>;
		pl022,interface = <0>;
		pl022,com-mode = <0>;
		spi-max-frequency = <40500000>;
	};

	spidev@3 {
		compatible = "spidev";
		reg = <3>;
		pl022,interface = <0>;
		pl022,com-mode = <0>;
		spi-max-frequency = <40500000>;
	};
};

&hisfc {
	hi_sfc {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <160000000>;
		m25p,fast-read;
	};
};

&hisnfc {
	hinand {
		compatible = "jedec,spi-nand";
		reg = <0>;
		spi-max-frequency = <160000000>;
	};
};

&hinfc610 {
	assigned-clocks = <&clock HI3531D_NFC_CLK>;
	assigned-clock-rates = <200000000>;

	hinand {
		compatible = "jedec,nand";
		reg = <0>;
		nand-max-frequency = <200000000>;
	};
};

&hidmac {
    status = "okay";
};

&mdio {
	ethphy: ethernet-phy@1 {
		reg = <1>;
	};
};

&higmac {
	phy-handle = <&ethphy>;
	phy-mode = "rgmii";
};

&pmux {
	i2c0_pmux: i2c0_pmux {
		pinctrl-single,pins = <
			0x01CC 0x1	/*I2C0_SDA*/
			0x01D0 0x1	/*I2C0_SCL*/
		>;
	};

	i2c1_pmux: i2c1_pmux {
		pinctrl-single,pins = <
			0x0148 0x2	/*I2C1_SDA*/
			0x014C 0x2	/*I2C1_SCL*/
		>;
