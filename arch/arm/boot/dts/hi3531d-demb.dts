			0x0098 0x0  /*VI4_CLK*/
			0x00bc 0x0  /*VI5_CLK*/
			0x00e4 0x0  /*VI6_CLK*/
			0x0108 0x0  /*VI7_CLK*/
		>;
	};
	vicap_demopro_pmux: vicap_demopro_pmux{
		pinctrl-single,pins = <
			0x0000 0x2  /*VI_ADC_REFCLK0*/
			0x0024 0x2  /*VI0_CLK*/
